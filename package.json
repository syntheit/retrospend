{
  "name": "retrospend",
  "version": "0.3.7",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node scripts/next-build.mjs",
    "check": "biome check .",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "prisma db push && next dev -p 1997",
    "postinstall": "prisma generate && mkdir -p public/images/flags && cp -n node_modules/circle-flags/flags/*.svg public/images/flags/ 2>/dev/null || true && mkdir -p public/images/crypto && cp -n node_modules/cryptocurrency-icons/svg/color/*.svg public/images/crypto/ 2>/dev/null || true && node scripts/generate-crypto-icons-list.mjs",
    "preview": "node scripts/next-build.mjs && next start",
    "start": "next start",
    "test": "vitest run",
    "ts-node": "ts-node --esm",
    "typecheck": "tsc --noEmit"
  },
